{
  "name": "vite-react-electron",
  "version": "0.0.0",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "concurrently -n=react,electron -c=green,blue \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "node -r ts-node/register script/dev-boot --env=development --watch",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/minimist": "^1.2.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "acorn": "^8.3.0",
    "chalk": "^4.1.1",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "electron": "^13.1.2",
    "minimist": "^1.2.5",
    "ora": "^5.4.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.2",
    "vite": "^2.1.5",
    "vitejs-plugin-electron": "^0.1.3"
  }
}
